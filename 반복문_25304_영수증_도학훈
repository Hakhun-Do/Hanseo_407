//반복문_25304_영수증_도학훈
#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main() {

	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	int total;
	cin >> total;
	int n, temp1, temp2, sum = 0;
	cin >> n;
	for (int i = 0; i < n; i++) {
		cin >> temp1 >> temp2;
		sum = sum + (temp1 * temp2);
	}
	if (total == sum)cout << "Yes";
	else cout << "No";
	return 0;
}
