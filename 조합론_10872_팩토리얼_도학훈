//조합론_10872_팩토리얼_도학훈
#include<iostream>
#include<vector>
using namespace std;

int main() {

    ios::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);

    int n, sum=1;
    cin >> n;
    for (int i = 0; i < n; i++) {
        sum = sum * (i + 1);
    }
    cout << sum;
    return 0;
}
 
