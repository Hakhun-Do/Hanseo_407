//반복문_10950_A+B-3_도학훈
#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main() {

	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	int num, temp1, temp2;
	cin >> num;
	vector<int> sum;
	for (int i = 0; i < num; i++) {
		cin >> temp1 >> temp2;
		sum.push_back(temp1 + temp2);
	}
	for (int i = 0; i < num; i++) {
		cout << sum[i] << "\n";
	}
	return 0;
}
