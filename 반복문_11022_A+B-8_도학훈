//반복문_11022_A+B-8_도학훈
#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main() {

	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	int n, temp1, temp2;
	vector<int> sum;
	cin >> n;
	for (int i = 0; i < n; i++) {
		cin >> temp1 >> temp2;
		sum.push_back(temp1 + temp2);
		cout << "Case #" << i+1 << ": "<<temp1<<" + "<<temp2<<" = " << sum[i] << "\n";
	}
	
	return 0;
}
