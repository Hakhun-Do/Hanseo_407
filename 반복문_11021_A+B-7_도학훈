//반복문_11021_A+B-7_도학훈
#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main() {

	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	int n, temp1, temp2;
	vector<int> sum;
	cin >> n;
	for (int i = 0; i < n; i++) {
		cin >> temp1 >> temp2;
		sum.push_back(temp1 + temp2);
	}
	for (int i = 1; i <= n; i++) {
		cout << "Case #" << i << ": " << sum[i - 1]<<"\n";
	}
	return 0;
}
