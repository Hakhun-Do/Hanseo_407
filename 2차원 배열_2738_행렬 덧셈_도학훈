//2차원 배열_2738_행렬 덧셈_도학훈
#include<iostream>
#include<vector>

using namespace std;
int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	int N, M;
	int temp=0;
	cin >> N >> M;
	vector<vector<int>> vector1(N, vector<int>(M));
	vector<vector<int>> vector2(N, vector<int>(M));
	for (int i = 0; i < N; i++) {
		for (int j = 0; j < M; j++) {
			cin >> temp;
			vector1[i][j]=temp;
		}
	}
	for (int i = 0; i < N; i++) {
		for (int j = 0; j < M; j++) {
			cin >> temp;
			temp += vector1[i][j];
			vector2[i][j] = temp;
		}
	}
	for (int i = 0; i < N; i++) {
		for (int j = 0; j < M; j++) {
			cout << vector2[i][j]<<" ";
		}
		cout << "\n";
	}
	return 0;
}
