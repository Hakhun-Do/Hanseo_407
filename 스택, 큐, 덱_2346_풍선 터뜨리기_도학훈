//스택, 큐, 덱_2346_풍선 터뜨리기_도학훈
#include<iostream>
#include<deque>
#include<vector>
#include<map>
using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);

	deque<int> deque;
	vector<int> vector;
	map<int, int> map;

	int N;
	cin >> N;
	int temp1, temp2;
	for (int i = 0; i < N; i++) {
		cin >> temp1;
		deque.push_back(temp1);
		map[temp1] = i + 1;
	}

	while (true) {
		if (deque.empty()) break;
		temp2 = deque.front();
		vector.push_back(map[deque.front()]);
		deque.pop_front();

		if () {
			
		}
		else {
			
		}
	}

	for (int i = 0; i < N; i++)cout << vector[i] << " ";
}
