import java.util.*;

public class Main {
    static long sum = 1;

    public static long dfs(int x) {
        if(x > 1) {
            sum *= x;
            dfs(x-1);
        }
        return sum;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();

        System.out.println(dfs(n));
    }
