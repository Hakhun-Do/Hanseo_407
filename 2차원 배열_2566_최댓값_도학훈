//2차원 배열_2566_최댓값_도학훈
#include<iostream>
#include<vector>

using namespace std;
int main() {

	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	int top = 0, point1, point2, temp;

	for (int i = 0; i < 9; i++) {
		for (int j = 0; j < 9; j++) {
			cin >> temp;
			if (top <= temp) {
				top = temp;
				point1 = i+1;
				point2 = j+1;
			}
		}
	}
	cout << top << "\n" << point1 << " " << point2;
	return 0;
}
